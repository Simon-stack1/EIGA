@model List<Film>

@{
  ViewData["Title"] = "Home Page";
}

<div class="text-center">
  <h1 class="display-4">Welcome</h1>

  <div class="products">
    @foreach (Film p in Model)
    {
      <div class="product">
        <p>@p.Name</p>
        <p>@p.Available</p>
        <p>@p.Price</p>
      </div>
    }
  </div>
</div>
